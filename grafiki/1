import numpy as np
import matplotlib.pyplot as plt
import math

a = float (input("Введите a "))
b = float (input("Введите b "))
n = float (input("Введите n "))

x = np.linspace(0, n, 50)
y = np.cos(x)**3 + a*x*np.sin(b*x)**2

plt.title('Линейная зависимость y = a + b*x^2 + c*x^0.5')
plt.xlabel('x')
plt.ylabel('y')
plt.grid() 
plt.plot(x, y)
plt.show()
